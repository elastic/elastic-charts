    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "ts-debounce": "^1.0.0",
    "utility-types": "^3.10.0",
    "uuid": "^3.3.2"
